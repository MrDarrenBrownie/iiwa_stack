<launch>
    <arg name="move_group" default="eef_light_static" />
    <arg name="sim" default="false" />
    <arg name="rviz" default="true" />
    <arg name="move" default="true" />
    <arg name="respawn" default="false" />
    <arg name="grab_frames" default="false" />
    <arg name="w_o_eef" default="false" />
    <arg name="w_eef_static" default="true" />
    <arg name="w_eef_adjustable" default="false" />

    <include file="$(find iiwa_tesla_publish)/launch/iiwa_tesla_launch_full.launch">
        <arg name="move_group" value="$(arg move_group)" />
        <arg name="sim" value="$(arg sim)" />
        <arg name="rviz" value="$(arg rviz)" />
        <arg name="w_o_eef" value="$(arg w_o_eef)" />
        <arg name="w_eef_static" value="$(arg w_eef_static)" />
        <arg name="w_eef_adjustable" value="$(arg w_eef_adjustable)" />
    </include>

    <!--
    <node ns="iiwa" name="add_objects_to_scene" pkg="iiwa_tesla_publish" type="add_objects_to_scene" output="screen" />
    -->
    
    <node if="$(arg move)" ns="iiwa" name="iiwa_tesla_move_route" pkg="iiwa_tesla_publish" type="move_route" respawn="$(arg respawn)" respawn_delay="10" output="screen" />
</launch>
